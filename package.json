{
  "name": "podlite-nextjs-site-blog-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next telemetry disable && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "yarn config set --home enableTelemetry 0 && next telemetry disable",
    "ci": "rimraf node_modules && yarn",
    "clean": "rimraf 'built/!(.gitignore)' 'public/assets/!(.gitignore)' .next out",
    "process": "yarn clean && mkdir -p built && ts-node -P tsconfig-node.json bin/makeDataSource.ts",
    "watch": "npx nodemon --watch ./data --watch ./pub -e pod6   --exec 'yarn clean && yarn process && next dev'",
    "format": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@podlite/schema": "^0.0.9",
    "@podlite/to-jsx": "^0.0.12",
    "@types/node": "17.0.17",
    "@types/react": "17.0.39",
    "commander": "^9.0.0",
    "eslint": "8.9.0",
    "eslint-config-next": "12.0.10",
    "imagemin-optipng": "^8.0.0",
    "iso_9": "^1.0.4",
    "newbase60": "^1.3.1",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.4",
    "next-optimized-images": "^2.6.2",
    "pod6": "^0.0.44",
    "podlite": "^0.0.18",
    "prettier": "^2.5.1",
    "react-modal": "^3.14.4",
    "rimraf": "^3.0.2",
    "sass": "^1.49.8",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "4.5.5"
  },
  "packageManager": "yarn@3.1.1"
}
